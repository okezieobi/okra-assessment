{
  "name": "okra-assessment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production DEBUG=okra-assessment:* node ./src/server.js",
    "build": "npx tsc",
    "dev": "NODE_ENV=development DEBUG=okra-assessment:* nodemon ./src/server.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.21.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cors": "latest",
    "@types/debug": "latest",
    "@types/express": "latest",
    "@types/morgan": "latest",
    "@types/node": "latest",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "*.{js,css,md,ts}": "prettier --write"
  }
}
